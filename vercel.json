{
  "version": 2,
  "name": "ub-api",
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node@1.11.0"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "main.js"
    },
    {
      "src": "/(.*)",
      "dest": "main.js"
    }
  ],
  "functions": {
    "api/*": {
      "memory": 512,   // Adjust memory allocation as needed
      "maxDuration": 30
    }
  }
}
